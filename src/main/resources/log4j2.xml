<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<Configuration status="INFO">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="STDOUT" level="ERROR"/>
        </Root>
    </Loggers>
</Configuration>
